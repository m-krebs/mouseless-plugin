{
    "manifest_version": 3,
    "name": "Mouseless",
    "version": "0.0.1",
    "description": "Quick keyboard access to links and other interactive elements",
    "icons": {
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "*://*/*"
            ]
        }
    ],
    "options_page": "settings/options.html"
}